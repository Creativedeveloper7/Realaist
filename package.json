{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:vercel": "npm ci && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clear-cache": "node scripts/clear-cache.js --all",
    "clear-build": "node scripts/clear-cache.js --build",
    "clear-npm": "node scripts/clear-cache.js --npm",
    "clear-vite": "node scripts/clear-cache.js --vite",
    "fresh-install": "node scripts/clear-cache.js --deps",
    "dev-clean": "npm run clear-cache && npm run dev",
    "build-clean": "npm run clear-cache && npm run build",
    "optimize": "node scripts/optimize-performance.js",
    "deploy": "npm run optimize && npm run build"
  },
  "dependencies": {
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@rollup/rollup-linux-x64-gnu": "^4.28.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "rollup": "^4.28.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
